@model IEnumerable<SportClub.Models.Accounting>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Client.FName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Month)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Payment)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ticket.NameTicket)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Client.FName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Month)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Payment)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ticket.NameTicket)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.AccountingID }) |
            @Html.ActionLink("Details", "Details", new { id=item.AccountingID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.AccountingID })
        </td>
    </tr>
}

</table>
